{
	"name": "node-express",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:yuta-katayama-23/node-express.git",
	"author": "yuta-katayama-23",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.87.0",
		"@aws-sdk/lib-dynamodb": "^3.87.0",
		"ajv": "^8.11.0",
		"ajv-formats": "^2.1.1",
		"app-root-path": "^3.0.0",
		"camelcase-keys": "^7.0.2",
		"config": "^3.3.7",
		"express": "^4.17.3",
		"express-openapi-validator": "^4.13.7",
		"lodash": "^4.17.21",
		"make-error": "^1.3.6",
		"snakecase-keys": "^5.4.1",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"babel-loader": "^8.2.5",
		"core-js": "^3.22.2",
		"eslint": "^8.14.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.1.5",
		"eslint-webpack-plugin": "^3.1.1",
		"jest": "^27.5.1",
		"lint-staged": "^12.4.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.2",
		"regenerator-runtime": "^0.13.9",
		"simple-git-hooks": "^2.7.0",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.2",
		"webpack-node-externals": "^3.0.0"
	},
	"description": "My webpack project",
	"scripts": {
		"build:dev": "webpack watch --node-env=development",
		"build:prod": "webpack --node-env=production",
		"build:debug": "webpack --node-env=development",
		"start:watch": "nodemon dist/index.js",
		"lint": "eslint src/*",
		"test": "jest",
		"mysqldump:schema": "docker exec -t mysql mysqldump -u root --databases user --no-data > ./sql/schema.sql",
		"mysqldump:data": "docker exec -t mysql mysqldump -u root user -t users > ./sql/data.sql",
		"create:table": "aws dynamodb create-table --cli-input-json file://./commands/create-table.json --endpoint-url http://localhost:8000",
		"prepare": "npx simple-git-hooks"
	},
	"lint-staged": {
		"*": "npx prettier --ignore-unknown --write"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	}
}
